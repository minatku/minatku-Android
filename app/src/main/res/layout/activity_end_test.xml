<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/base_page"
    tools:context=".ui.endtest.EndTestActivity">

    <ImageView
        android:id="@+id/tv_minatku"
        android:layout_width="170dp"
        android:layout_height="21dp"
        app:layout_constraintBottom_toTopOf="@+id/iv_starttest"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/minatku_text_black" />

    <ImageView
        android:id="@+id/iv_starttest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="104dp"
        app:layout_constraintBottom_toTopOf="@+id/tv_taglinestarttest"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/success_img" />

    <ImageView
        android:id="@+id/tv_taglinestarttest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="48dp"
        app:layout_constraintBottom_toTopOf="@+id/btn_starttest"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.22"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/tagline_selesai" />

    <Button
        android:id="@+id/btn_starttest"
        android:layout_width="363dp"
        android:layout_height="58dp"
        android:layout_marginBottom="84dp"
        android:background="@drawable/rounded"
        android:backgroundTint="@color/base_page"
        android:fontFamily="@font/dm_sans_bold"
        android:text="@string/masuk_ke_home"
        android:textColor="@color/white"
        android:textSize="18sp"
        app:iconTint="#020202"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
